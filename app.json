{
  "expo": {
    "scheme": "spotify",
    "name": "Ox",
    "slug": "Spotify",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/oxicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jdnvn.ox"
    }
  }
}
